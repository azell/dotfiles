Host github.com
  HostName github.com
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_ed25519

Host work.github.com
  HostName github.com
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/idj_ed25519

Host gitlab.com
  HostName gitlab.com
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_ed25519

